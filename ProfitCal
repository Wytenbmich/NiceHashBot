import requests
import json
from bs4 import BeautifulSoup
def findProfit():
	url = "https://whattomine.com/coins/115.json"
	response = requests.get(url)
	cont = json.loads(response.content)
	difficulty = cont['difficulty']
	"Finds difficulty"
	url = "https://whattomine.com/coins/115-dgb-qubit?utf8=%E2%9C%93&hr=1000000.0&d_enabled=true&d=" + str(difficulty) + "&p=1700.0&fee=0.0&cost=0.1&hcost=0.0&commit=Calculate"
	response = requests.get(url)
	cont = BeautifulSoup(response.content, "html.parser")
	table = cont.findAll("td", attrs={"class" : "text-right text-monospace"})
	rows = []
	for row in table:
		rows.append(row)
	print(difficulty)
	revenue = rows[8].text
	return revenue

print(findProfit())